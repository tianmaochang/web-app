		$('#tip').html('');

		var name = $.trim($('#name').val());
		var email = $.trim($('#email').val());
		var remark = $.trim($('#remark').val());
		$('#tip').show();
		<%--location.href = "https://www.focuswinecellars.com/thank-you.php";--%>

		if(name=='') {
			$('#name').focus();
			$('#tip').append('请填写姓名字段。.<br>');
			return false;
		}
		if(email=='') {
			$('#email').focus();
			$('#tip').append('请填写电子邮件字段。<br>');
			return false;
		}
		if(isEmail(email)==false) {
			$('#email').focus();
			$('#tip').append('请输入有效的电子邮件地址。<br>');
			return false;
		}
		if(remark=='') {
			$('#remark').focus();
			$('#tip').append('请填写留言栏<br>');
			return false;
		}

		var frm = $('#conForm');
		$.ajax({
			type: 'post',
			url: '${base }/web/user/create',
			data: frm.serialize(),
			success: function (data) {
				$('#tip').html('提交成功<br>');
				$('#name').html('');
				$('#company').html('');
				$('#phone').html('');
				$('#email').html('');
				$('#remark').html('');
			},
			error: function (data) {
				$('#tip').html('提交失败，请重试<br>'+data);
			},
		});
		return false;
	};
</script>
<script src="${baseStatic}/js/rocket-loader.min.js" data-cf-nonce=""></script>
</html>
